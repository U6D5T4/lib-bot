{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "BotConfiguration": {
    "BotToken": "{BotToken}",
    "HostAddress": "{HostAddress}"
  },
  "DbConfiguration": {
    "AuthToken": "{AuthToken}",
    "BasePath": "{BasePath}"
  },
  "EmailConfiguration": {
    "DisplayName": "LibBot",
    "Host": "webmail.itechart-group.com ",
    "Port": 25
  },
  "BotCredentials": {
    "Login": "{Bot_username}",
    "Password": "{Bot_password}"
  },
  "SharePointConfiguration": {
    "BaseAddress": "{Base address}",
    "MailDomain": "{Domain of mail}" 
  } 
}
