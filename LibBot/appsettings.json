{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "BotConfiguration": {
    "BotToken": "{BotToken}",
    "HostAddress": "{HostAddress}"
  },
  "DbConfiguration": {
    "AuthToken": "{AuthToken}",
    "BasePath": "{BasePath}"
  },
  "EmailConfiguration": {
    "Email": "{Bot_email}",
    "Password": "{Bot_password}",
    "DisplayName": "LibBot",
    "Host": "smtp-mail.outlook.com",
    "Port": 587
  }
}
