{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "BotConfiguration": {
    "BotToken": "{BotToken}",
    "HostAddress": "{HostAddress}"
  },
  "DbConfiguration": {
    "BasePath": "{BasePath}"
  },
  "EmailConfiguration": {
    "DisplayName": "LibBot",
    "Host": "{Host}",
    "Port": "{Port}"
  },
  "BotCredentials": {
    "Login": "{Bot_username}",
    "Password": "{Bot_password}"
  },
  "SharePointConfiguration": {
    "BaseAddress": "{BaseAddress}",
    "MailDomain": "{Domain of mail}"
  },
  "AuthDbConfiguration": {
    "Login": "{Login}",
    "Password": "{Password}",
    "BaseAddress": "{BaseAddress}",
    "RefreshAddress": "{RefreshAddress}"
  }
}